{
	"__name__": "Static",

	"_id": {
		"__type__": "uuid"
	},

	"_created": {
		"__type__": "timestamp",
		"__optional__": true
	},

	"_updated": {
		"__type__": "timestamp",
		"__optional__": true
	},

	"key": {
		"__type__": "string",
		"__minimum__": 3,
		"__maximum__": 16,
		"__regex__": "^[a-z_]{3,16}"
	},

	"content": {
		"__type__": "string",
		"__maximum__": 65535
	}
}